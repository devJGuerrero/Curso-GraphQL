{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "lib": ["dom","es6"],
    "outDir": "./build/",
    "rootDir": "./src/",
    "removeComments": true,
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  }
}
