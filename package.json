{
  "name": "curso-graphql",
  "version": "1.0.0",
  "description": "Curso GraphQL Udemy",
  "repository": "https://github.com/devJGuerrero/Curso-GraphQL.git",
  "author": "Jos√© Guerrero <jose.guerrero.carrizo@gmail.com>",
  "license": "MIT",
  "main": "build/server.js",
  "scripts": {
    "start": "node build/main.js",
    "build": "tsc -p . && ncp src/graphql build/graphql",
    "start:dev": "yarn run build:dev",
    "build:dev": "nodemon \"src/main.ts\" --exec \"ts-node\" src/main.ts -e ts,graphql"
  },
  "dependencies": {
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-graphql": "^0.11.0",
    "global": "^4.4.0",
    "graphql": "^15.3.0",
    "graphql-import-node": "^0.0.4",
    "graphql-playground-middleware-express": "^1.7.18",
    "graphql-tools": "^6.0.18",
    "http": "^0.0.1-security",
    "ncp": "^2.0.0",
    "ts-node": "^8.10.2",
    "typescript": "^4.0.2"
  },
  "devDependencies": {
    "@types/compression": "^1.7.0",
    "@types/cors": "^2.8.7",
    "@types/express": "^4.17.7",
    "@types/express-graphql": "^0.9.0",
    "@types/graphql": "^14.5.0",
    "@types/node": "^14.6.0"
  }
}
